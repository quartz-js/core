{
  "name": "@railken/quartz-core",
  "version": "2.0.1",
  "author": "railken@protonmail.ch",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:lib",
    "lint": "vue-cli-service lint",
    "build:lib": "vue-cli-service build --target lib --name index src/index.js",
    "test:unit": "vue-cli-service test:unit  --no-cache",
    "release": "npm run build && npm publish --access=public"
  },
  "dependencies": {
    "js-beautify": "^1.8.9",
    "json-formatter-js": "^2.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "string": "^3.3.3",
    "vue": "^2.5.0",
    "vue-clickaway": "^2.2.2",
    "vue-codemirror": "^4.0.6",
    "vue-js-toggle-button": "^1.3.1",
    "vue-resource": "^1.2.1",
    "vuetify": "^1.4.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-plugin-unit-jest": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "bugs": {
    "url": "https://github.com/railken/quartz-core/issues"
  },
  "homepage": "https://github.com/railken/quartz-core#readme",
  "license": "MIT",
  "main": "dist/index.umd.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/railken/quartz-core.git"
  }
}
